  <wsdl:message name="hello">
    <wsdl:part element="tns:hello" name="parameters" />
  </wsdl:message>
  <wsdl:message name="goodbyeResponse">
    <wsdl:part element="tns:goodbyeResponse" name="parameters" />
  </wsdl:message>
  <wsdl:message name="helloResponse">
